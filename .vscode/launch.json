{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Extension",
            "type": "extensionHost",
            "request": "launch",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "outFiles": [
                "${workspaceFolder}/extension/**/*.js"
            ],
            "preLaunchTask": "tsc: watch"
        },
        {
            "name": ".NET Core Debugger (launch)",
            "type": "coreclr",
            "request": "launch"
        }
    ]
}